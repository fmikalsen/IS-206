the_count = [1, 2, 3, 4, 5]
fruits = ["apples", "oranges", "pears", "apricots"]
change = [1, "pennies", 2, "dimes", 3, "quarters"]

for number in the_count:
	print "this is the count %d" % number

for fruit in fruits:
	print "A fruit of the type: %s" % fruit

for i in change:
	print "I got %r" % i

elements = []

#this is a way to do it without
i = range(0, 6)
print "Adding %s to the list." % i
elements.append(i)

for i in elements:
	print "Element was: %s" % i
